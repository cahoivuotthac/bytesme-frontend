{
  "expo": {
    "name": "pie-frontend",
    "slug": "bytesme",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.bytesme.frontend",
			"googleServicesFile": "./google-services.json"
    },
    "fonts": [
      {
        "asset": "./assets/fonts/Inter-Regular.ttf",
        "family": "Inter-Regular"
      },
      {
        "asset": "./assets/fonts/Inter-Medium.ttf",
        "family": "Inter-Medium"
      },
      {
        "asset": "./assets/fonts/Inter-Bold.ttf",
        "family": "Inter-Bold"
      },
      {
        "asset": "./asset/Inter-SemiBold.ttf",
        "family": "Inter-SemiBold"
      }
    ],
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "1368ad0d-e938-411a-ab99-4624613b6ad7"
      }
    },
    "owner": "dxcmint868"
  }
}
